page {
    position: relative;
    background-color: #f2f2f2;
}

.nav {
    position: absolute;
    top: 340rpx;
    width: 100%;
    z-index: 90;
}

.nav-scr {
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.ti-btn::after {
    border: 1rpx solid #02b3b9;
}

.fs40 {
    font-size: 40rpx;
}

.bdl {
    border-left: 1rpx solid #ebebeb;
}

.fs401 {
    font-size: 20rpx;
}

.overflow:nth-child(1) .line::after {
    content: "";
    display: block;
    width: 0;
    height: 0;
}

.overflow:nth-last-child(1) .line::before {
    content: "";
    display: block;
    width: 0;
    height: 0;
}

.line::after {
    content: "";
    display: block;
    position: absolute;
    top: -79rpx;
    left: 2;
    width: 2rpx;
    height: 80rpx;
    background-color: #ebebeb;
}

.line::before {
    content: "";
    display: block;
    position: absolute;
    bottom: -79rpx;
    left: 2;
    width: 2rpx;
    height: 80rpx;
    background-color: #ebebeb;
}

.overflow {
    overflow: hidden;
}

.none {
    width: 100%;
    height: 600rpx;
    line-height: 600rpx;
    box-sizing: border-box;
    padding: 0 18rpx;
    text-align: center;
    margin-top: 50rpx;
}

.none image {
    width: 200rpx;
    height: 200rpx;
}