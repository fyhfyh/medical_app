page {
    background: #F1F1F1;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.boxcontainer {
    width: 100%;
    padding: 20rpx;
}

.boxcontainer-list {
    width: 100%;
    height: 280rpx;
    overflow: hidden;
    position: relative;
}

.boxcontainer-bgt {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
}

.boxcontainer-bgs {
    width: 100%;
    height: 100%;
    opacity: 0.6;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 5;
}

.boxcontainer-con {
    width: 100%;
    height: 100%;
    padding: 50rpx;
    position: relative;
    display: flex;
    justify-content: space-between;
    align-items: center;
    z-index: 30;
}

.boxcontainer-conl {
    width: 135rpx;
    height: 135rpx;
}

.boxcontainer-conr {
    width: 75%;
}

.boxcontainer-conr view {
    width: 100%;
    text-align: right;
    color: #fff;
}

.boxcontainer-conr view:nth-child(1) {
    font-size: 36rpx;
    margin-bottom: 10rpx;
}

.boxcontainer-conr view:nth-child(2) {
    font-size: 26rpx;
    letter-spacing: 3rpx;
    line-height: 45rpx;
}

.boxcontainer-conr.left view {
    text-align: left;
}

.boxcontainer-step {
    color: #fff;
    background: rgba(255,255,255,0.5);
    font-size: 24rpx;
    padding: 5rpx 50rpx;
    position: absolute;
    top: 15rpx;
    left: -45rpx;
    z-index: 50;
    transform: rotate(-45deg);
}

.listboxs {
    width: 100%;
    padding: 20rpx;
}

.listboxsscroll {
    width: 100%;
    padding: 30rpx;
    background: #fff;
    border-radius: 10rpx;
}

.scrollss {
    display: inline-block;
}

.scrolllist {
    width: 125rpx;
    height: 100rpx;
    display: inline-block;
    text-align: center;
}

.scrolllist view:first-child {
    width: 75rpx;
    height: 75rpx;
    border-radius: 50%;
    margin: 0 auto;
    padding: 15rpx;
    display: inline-block;
}

.scrolllist view:last-child {
    text-align: center;
    font-size: 28rpx;
    color: #8C8D94;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 10rpx;
}

.scrolllist-xian {
    display: inline-block;
    width: 75rpx;
    border-bottom: 5rpx #BEC0C5 dashed;
    position: relative;
    top: -80rpx;
}

.scrolllist view.onon {
    color: #000;
}

.listboxscy {
    width: 100%;
    background: #fff;
    margin-top: 20rpx;
}

.listboxscy-aa {
    width: 100%;
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
    padding-bottom: 10rpx;
    border-bottom: 20rpx solid #f1f1f1;
}

.listboxscy-aal {
    width: 25%;
}

.listboxscy-aal view {
    width: 100%;
    text-align: right;
}

.listboxscy-aal view:nth-child(2) {
    font-size: 28rpx;
    color: #2F2F2F;
    margin: 10rpx 0 15rpx;
}

.listboxscy-aal view image {
    width: 50rpx;
    height: 50rpx;
}

.listboxscy-aar {
    width: 75%;
}

.listboxscy-aar view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.listboxscy-aar view:nth-child(1) {
    font-size: 35rpx;
    color: #2F2F2F;
}

.listboxscy-aar view:nth-child(2) {
    font-size: 26rpx;
    color: #2F2F2F;
    margin: 12rpx 0 20rpx;
}

.listboxscy-aar view:nth-child(3) image {
    width: 90px;
    height: 108rpx;
    margin-right: 20rpx;
}

.addlist {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 4%;
    right: 4%;
    overflow: hidden;
    background: #fff;
}

.addlist image {
    width: 100%;
    height: 100%;
}

.modalbtns {
    width: 100vw;
    height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
}

.modalbtns-empty {
    width: 100vw;
    height: 100vh;
    background: rgba(0,0,0,0.5);
}

.modalbtns--but {
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 20rpx;
}

.modalbtns--but view {
    width: 100%;
    text-align: center;
    background: #fff;
    padding: 20rpx;
    font-size: 30rpx;
    border-radius: 10rpx;
    margin-top: 10rpx;
}

.modalbtns--but view:last-child {
    margin-top: 20rpx;
}