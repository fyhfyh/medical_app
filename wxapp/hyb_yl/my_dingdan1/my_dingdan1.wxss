page {
    position: relative;
    background-color: #f2f2f2;
}

.tiwen-bg {
    width: 100%;
    height: 50rpx;
}

.nav {
    position: absolute;
    top: 19rpx;
    width: 100%;
    z-index: 2;
}

.nav-scr {
    height: 142rpx;
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.ding-btn {
    padding: 10rpx 20rpx;
    border: 1rpx solid #ebebeb;
}

.ding-btn-hover {
    padding: 10rpx 30rpx;
    border: 1rpx solid #02b3b9;
}

.none {
    width: 100%;
    height: 600rpx;
    line-height: 600rpx;
    box-sizing: border-box;
    padding: 0 18rpx;
    text-align: center;
    margin-top: 50rpx;
}

.none image {
    width: 200rpx;
    height: 200rpx;
}