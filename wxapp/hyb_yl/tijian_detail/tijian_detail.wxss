page {
    position: relative;
    background-color: #f2f2f2;
}

.swiper_wrap {
    height: 400rpx;
}

.taocan {
    width: 200rpx;
    height: 56rpx;
}

.zhuan-tui {
    width: 160rpx;
    height: 36rpx;
    line-height: 36rpx;
    text-align: center;
}

.zhuan-tui::after {
    content: "";
    display: block;
    position: absolute;
    bottom: -65%;
    left: 15%;
    border: 14rpx solid;
    border-color: #ebebeb transparent transparent transparent;
}

.l {
    justify-content: flex-start;
}

.c {
    justify-content: center;
}

.r {
    justify-content: flex-end;
}

.bdb.bdb2:nth-last-child(1) {
    border: none;
}

.sub_nav {
    border-bottom: 2rpx solid #f1f1f1;
    height: 70rpx;
    line-height: 70rpx;
    background: #f0f0f0;
    margin: 20rpx 0;
    margin-bottom: 0rpx;
}

.sub_nav text {
    font-size: 28rpx;
    padding: 0 20rpx;
    color: #242424;
    display: inline-block;
    height: 100%;
    position: relative;
}

.onon {
    border-left: 2rpx solid #ebebeb;
}

.onon:nth-child(1) {
    border-left: none;
}

swiper {
    height: 100%;
    min-height: 300rpx;
}

.detail-footer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    border-top: 2rpx solid #ebebeb;
}

.detail-footer button {
    background-color: transparent;
}

.detail-footer button::after {
    border: none;
}

.bdl {
    border-left: 2rpx solid #ebebeb;
}

.w60 {
    width: 60rpx;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
    z-index: 2;
    overflow: hidden;
}

.pay {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    min-height: 300rpx;
    z-index: 5;
}

.add-btn {
    border: 2rpx solid #9698a4;
}

.add {
    width: 30rpx;
    height: 30rpx;
}

.biaoqian1 {
    border: 2rpx solid #ebebeb;
}

.h38 {
    height: 38rpx;
}

.bd {
    border: 2rpx solid #02b3b9;
}

.del {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 600rpx;
    height: 400rpx;
    transform: translate(-50%,-50%);
    background-color: #fff;
    z-index: 12;
}

.btn {
    width: 40%;
    height: 88rpx;
}

.w528 {
    width: 528rpx;
}

.liao-input {
    width: 85%;
    height: 66rpx;
    background-color: #f5f5f5;
    border: 2rpx solid #e4e4e4;
}

.fa-btn {
    display: inline-block;
    width: 85rpx;
    height: 54rpx;
    line-height: 54rpx;
}

.bdbn:nth-last-child(1) {
    border-bottom: none;
}

.w25 {
    width: 25%;
}

.h600 {
    height: 400rpx;
}

.return-top {
    position: fixed;
    right: 5%;
    bottom: 20%;
    width: 120rpx;
    height: 120rpx;
}

.pb122 {
    padding-bottom: 122rpx;
}