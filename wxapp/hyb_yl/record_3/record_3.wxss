.mh180 {
    min-height: 180rpx;
}

.line::after {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    right: -60%;
    transform: translate(0,-50%);
    width: 50rpx;
    height: 2rpx;
    background-color: #06AC85;
}

.mr80:nth-last-child(1) .line::after {
    display: none;
}

.mr80:nth-last-child(1) {
    margin-right: 0;
}

.del {
    top: 0;
    right: 0;
    width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    background-color: #ff0000;
}

.mh412 {
    min-height: 412rpx;
}

.mr25 {
    margin-right: 90rpx;
}

.mr25:nth-child(3n) {
    margin-right: 0;
}

radio {
    transform: scale(0.8);
}

checkbox {
    transform: scale(0.8);
}