}

.mt60 {
    margin-top: 60rpx;
}

.none {
    color: #9698a4;
    background-color: #fff;
}

.nav {
    position: absolute;
    top: 290rpx;
    width: 100%;
    z-index: 2;
}

.nav-scr {
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.mt132 {
    margin-top: 132rpx;
}

.bdbn:nth-last-child(1) {
    border-bottom: none;
}

.fs40 {
    font-size: 40rpx;
}

.overflow {
    overflow: hidden;
}

.overflow:nth-child(1) .line::before {
    content: "";
    display: block;
    width: 0;
    height: 0;
}

.overflow:nth-last-child(1) .line::after {
    content: "";
    display: block;
    width: 0;
    height: 0;
}

.line::after {
    content: "";
    display: block;
    position: absolute;
    top: 0rpx;
    left: 2;
    width: 2rpx;
    height: 220rpx;
    background-color: #ebebeb;
}

.line::before {
    content: "";
    display: block;
    position: absolute;
    bottom: 0rpx;
    left: 2s;
    width: 2rpx;
    height: 220rpx;
    background-color: #ebebeb;
}