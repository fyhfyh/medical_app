page {
    background: #fff;
}

view {
    box-sizing: border-box;
}

.lf {
    float: left;
}

.rt {
    float: right;
}

.tab-list {
    width: 100%;
    padding: 30rpx 20rpx;
    overflow: hidden;
    background: #eee;
    border-bottom: 1rpx solid #afafaf;
}

.tab-list text:first-child {
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
    float: left;
}

.tab-list image {
    width: 25rpx;
    height: 15rpx;
    float: right;
    margin-top: 15rpx;
    margin-left: 10rpx;
}

.tab-list text:last-child {
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
    float: right;
}

.box {
    width: 100%;
    padding: 20rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #eee;
}

.box view {
    font-size: 28rpx;
    color: #000;
    letter-spacing: 3rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.box .lf {
    width: 80%;
}

.box .lf view:last-child {
    color: #afafaf;
    font-size: 22rpx;
}

.box .rt {
    width: 19%;
    text-align: right;
    font-size: 26rpx;
    margin-top: 12rpx;
}