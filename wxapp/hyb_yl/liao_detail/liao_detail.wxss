::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

image {
    width: 100%;
    height: 100%;
}

page {
    background-color: #EBEBEB;
}

.historycon {
    height: 93vh;
    width: 100%;
    flex-direction: column;
    display: flex;
    overflow: hidden;
    border-top: 0px;
}

.history {
    height: 93vh;
    font-size: 14px;
    line-height: 40px;
    word-break: break-all;
}

.h_item {
    margin-top: 20rpx;
}

.clearfloat:after {
    display: block;
    clear: both;
    content: "";
    visibility: hidden;
    height: 0;
}

.clearfloat {
    zoom: 1;
}

.tab {
    padding: 20rpx 20rpx 40rpx 50rpx;
    height: 20%;
    background-color: white;
}

.tab .tent {
    font-size: 33rpx;
    margin-bottom: 30rpx;
}

.new_imgtent {
    width: 190rpx;
    margin-top: 20rpx;
    position: relative;
    background: #EBEBEB;
    z-index: 2;
}

.new_imgtent image {
    border: 2rpx solid #DCDCDC;
}

.tzm::after {
    content: "";
    display: block;
    width: 30rpx;
    height: 30rpx;
    transform: rotate(45deg);
    position: absolute;
    left: -10rpx;
    top: 15rpx;
    background: transparent;
    border: 2rpx solid #DCDCDC;
    z-index: -1;
}

.tym::after {
    content: "";
    display: block;
    width: 30rpx;
    height: 30rpx;
    transform: rotate(45deg);
    position: absolute;
    right: -10rpx;
    top: 15rpx;
    background: transparent;
    border: 2rpx solid #DCDCDC;
    z-index: -1;
}

.tab .fabu {
    font-size: 33rpx;
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}

.xiahuaxia {
    width: 80%;
    text-align: center;
    margin: 0 auto;
    position: relative;
    top: 60rpx;
}

.time {
    border-radius: 10rpx;
    text-align: center;
    padding: 5rpx 10rpx;
    font-size: 24rpx;
    background-color: #DADADA;
    color: #fff;
}

.new_top_txt {
    width: 50%;
    position: relative;
    top: 38rpx;
    text-align: center;
    margin: 0 auto;
    font-size: 30rpx;
    color: #787878;
    background-color: #f7f7f7;
}

.news {
    text-align: center;
}

.img_null {
    height: 60rpx;
}

.l {
    height: 5rpx;
    width: 20%;
    margin-top: 30rpx;
    color: #000;
}

.page_row {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    align-items: flex-start;
}

.my_right {
    text-align: left;
    position: relative;
    right: 20rpx;
}

.you_left {
    text-align: left;
    position: relative;
    left: 20rpx;
}

.new_img {
    width: 80rpx;
    height: 80rpx;
    border-radius: 20rpx;
}

.new_txt {
    max-width: 380rpx;
    display: block;
    border-radius: 7px;
    line-height: 1.8em;
    background-color: #95d4ff;
    padding: 10rpx 30rpx 10rpx 30rpx;
    position: relative;
    border: 2rpx solid #DCDCDC;
}

.youm::after {
    content: "";
    display: block;
    width: 30rpx;
    height: 30rpx;
    transform: rotate(45deg);
    position: absolute;
    right: -10rpx;
    top: 15rpx;
    background: #95d4ff;
    z-index: 0;
}

.zuom::after {
    content: "";
    display: block;
    width: 30rpx;
    height: 30rpx;
    transform: rotate(45deg);
    position: absolute;
    left: -10rpx;
    top: 15rpx;
    background: #fff;
    z-index: 0;
}

.my {
    border-color: transparent transparent transparent #95d4ff;
}

.you {
    border-color: transparent #95d4ff transparent transparent;
}

.bot {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    transition: all 0.5s;
    z-index: 55;
}

.bot.close {
    bottom: -230rpx;
}

.modal {
    width: 100vw;
    height: 100vh;
    position: fixed;
    bottom: 0;
    left: 0;
}

.sendmessage {
    background-color: #F5F5F7;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: flex-end;
    padding: 20rpx 0;
    border-top: 2rpx solid #D9D9DB;
    border-bottom: 2rpx solid #D9D9DB;
}

.sendmessage textarea {
    width: 70%;
    border-radius: 10rpx;
    margin: 0 20rpx;
    margin-right: 10rpx;
    min-height: 50rpx;
    height: 50rpx;
    line-height: 1.6em;
    background-color: #F5F5F7;
    font-size: 28rpx;
    border-bottom: 1px solid #DDDDDF;
    padding: 10rpx;
}

.sendmessage button {
    border: 1px solid white;
    width: 18%;
    height: 70rpx;
    background: green;
    color: #fff;
    line-height: 70rpx;
    font-size: 26rpx;
}

.jia_img {
    height: 50rpx;
    width: 50rpx;
    margin-left: 20rpx;
    margin-bottom: 2px;
}

.bot_bottom {
    height: 230rpx;
    background: #F5F5F7;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.bot_bottom .bottom_item {
    margin-top: 20rpx;
    margin-left: 20rpx;
    text-align: center;
    width: 120rpx;
    height: 120rpx;
    font-size: 28rpx;
}

.bot_bottom .bottom_item .b_i_img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 30rpx;
    background: #FBFBFB;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    border: 2rpx solid #D9D9DB;
    margin-bottom: 10rpx;
}

.bot_bottom .bottom_item .b_i_img image {
    width: 60rpx;
    height: 50rpx;
}