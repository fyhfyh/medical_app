page {
    position: relative;
    height: 100%;
    background-color: #f2f2f2;
}

.h88 {
    height: 88rpx;
}

.h50 {
    height: 50rpx;
}

.mtn30 {
    margin-top: -30rpx;
}

.box-shaw {
    box-shadow: 0 0 24rpx rgba(0,0,0,0.1);
}

.cir {
    position: relative;
    width: 32rpx;
    height: 32rpx;
    border: 2rpx solid #c2c2c2;
    border-radius: 50%;
}

.cir.hover {
    border-color: #02b3b9;
}

.cir.hover::after {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 16rpx;
    height: 16rpx;
    background-color: #02b3b9;
    border-radius: 50%;
}

.bdceb {
    border: 2rpx solid #ebebeb;
}

.zi3 {
    z-index: 3;
}

.h54 {
    height: 54rpx;
}

.w144 {
    width: 144rpx;
}