page {
    position: relative;
    background-color: #f2f2f2;
}

.h120 {
    height: 120rpx;
}

.cir {
    width: 32rpx;
    height: 32rpx;
    border: 2rpx solid #c1c1c1;
}

.cir.hover {
    width: 32rpx;
    height: 32rpx;
    border: 2rpx solid #06AC85;
}

.cir.hover::after {
    content: '';
    display: block;
    border: 8rpx solid #06AC85;
    border-radius: 50%;
}

.bdb2:nth-last-child(1) {
    border-bottom: 0;
}

.h98 {
    height: 98rpx;
}

.contrast-btn {
    bottom: 8%;
    left: 0;
}