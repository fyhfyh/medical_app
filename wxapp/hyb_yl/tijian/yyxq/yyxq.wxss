@import "../tjxq/tjxq.wxss";

page {
    background-color: #f5f5f5;
}

.yyxqtop {
    width: 100%;
    padding: 3% 0% 0;
}

.yynews {
    width: 88%;
    margin: 0 auto;
    overflow: hidden;
    background-color: #fff;
    border-radius: 8px;
    padding: 5% 3%;
}

.yylogo {
    width: 70px;
    height: 70px;
}

.yylogo image {
    border-radius: 50%;
}

.yyxinxi {
    width: 60%;
    margin-left: 2%;
}

.yyxinxi01 {
    width: 100%;
    font-size: 18px;
    color: #242424;
    font-weight: bold;
}

.yyaddr {
    width: 100%;
    font-size: 16px;
    color: #5a5a5a;
    margin: 4% 0 3%;
}

.yyxinxi03 {
    overflow: hidden;
    width: 100%;
}

.yyxinxi03 text {
    display: inline-block;
    padding: 0 1%;
    border: 1px solid #44bcbc;
    border-radius: 3px;
    color: #44bcbc;
    margin-right: 2%;
    font-size: 12px;
    margin-bottom: 1%;
}

.yydingwei {
    width: 12%;
    margin-top: 4%;
}

.yydingwei .yydwimg {
    width: 25px;
    height: 25px;
    margin: 0 auto 2%;
}

.yydingwei .yydwtxt {
    width: 100%;
    text-align: center;
    font-size: 14px;
    color: #595959;
}