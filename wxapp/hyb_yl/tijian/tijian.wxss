page {
    background-color: #f5f5f5;
}

.tjtop {
    width: 100%;
    height: 150px;
}

.tjtc {
    width: 94%;
    padding: 5% 3% 0;
}

.tjtctitle {
    overflow: hidden;
    width: 100%;
}

.tjtctitleimg {
    width: 24px;
    height: 24px;
    margin-right: 2%;
}

.tjtctxt {
    font-size: 18px;
    color: #595959;
}

.tjtcsort {
    width: 100%;
    overflow: hidden;
    padding: 4% 0%;
}

.tjtcsortone {
    width: 48%;
    margin-bottom: 3%;
    margin-right: 4%;
    background-color: #fff;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
}

.tjtcsort .tjtcsortone:nth-child(2),.tjtcsort .tjtcsortone:nth-child(4) {
    margin-right: 0;
}

.sortoneimg {
    width: 100%;
    height: 90px;
}

.sortoneimg image {
    border-radius: 8px;
}

.sortonetxt {
    width: 100%;
    text-align: center;
    font-size: 16px;
    color: #242424;
    margin-top: 5%;
}

.fujin {
    width: 100%;
    padding: 4% 0;
}

.fujineve {
    width: 94%;
    padding: 4% 3%;
    overflow: hidden;
    position: relative;
    background-color: #fff;
    border-radius: 5px;
    margin-bottom: 3%;
}

.fujineve .fujineveimg {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    margin-right: 3%;
    margin-top: 1%;
}

.fujineve .fujinevename {
    width: 70%;
}

.fjjuli {
    position: absolute;
    bottom: 12%;
    right: 5%;
    color: #b5b5b5;
    font-size: 14px;
}

.fujinevename view:nth-child(1) {
    font-size: 20px;
    color: #242424;
    margin-bottom: 5%;
    margin-top: 2%;
}

.fujinevename view:nth-child(2) {
    font-size: 16px;
    color: #8c8c8c;
}

.yyjlicon {
    width: 65px;
    height: 65px;
    border-radius: 50%;
    position: fixed;
    bottom: 8%;
    right: 3%;
}

.yyjlicon image {
    border-radius: 50%;
}