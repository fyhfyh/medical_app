.num {
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
    font-size: 28rpx;
    border-bottom: 2rpx solid #efefef;
}

.c9 {
    color: #999;
    margin-right: 5rpx;
}

.c0 {
    color: #000;
}

.c {
    color: #0bac98;
}

.df {
    display: flex;
    align-items: center;
}

.symptom {
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
    font-size: 28rpx;
    border-bottom: 15rpx solid #efefef;
}

.symptom button {
    height: 70rpx;
    line-height: 70rpx;
    background-color: transparent;
    color: #0bac98;
}

.symptom button::after {
    border-color: #0bac98;
}

.tac {
    margin-top: 20rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
}

.dffe {
    display: flex;
    justify-content: flex-end;
    font-size: 28rpx;
    color: #999;
    box-sizing: border-box;
    padding: 10rpx 24rpx;
    border-bottom: 15rpx solid #efefef;
}