.list {
    padding: 20rpx;
}

.item {
    border: 2rpx solid #f0f0f0;
    padding: 20rpx;
    margin-bottom: 20rpx;
    font-size: 30rpx;
    border-radius: 10rpx;
    position: relative;
}

.date {
    position: absolute;
    right: 20rpx;
}

.list_item {
    position: relative;
    border-right: 2rpx solid #f0f0f0;
}

.list_item .right {
    position: absolute;
    right: 20rpx;
}