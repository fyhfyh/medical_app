page {
    background: #fff;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.lf {
    float: left;
}

.rt {
    float: right;
}

.doctor {
    width: 100%;
    padding: 30rpx 20rpx;
    background: #fff;
    overflow: hidden;
    border-bottom: 1rpx solid #f4f4f4;
    display: flex;
}

.doctor-img {
    width: 130rpx;
    height: 130rpx;
}

.doctor-img image {
    border-radius: 50%;
}

.d_left {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.df {
    display: flex;
    align-items: center;
}

.doctor-title {
    padding-left: 20rpx;
}

.doctor-title view {
    width: 100%;
    letter-spacing: 3rpx;
    font-size: 28rpx;
    color: #242424;
}

.doctor-title view:nth-child(1) text {
    display: inline-block;
}

.doctor-title view:nth-child(1) text:last-child {
    padding: 3rpx 10rpx;
    color: #FA8D87;
    font-size: 22rpx;
    border-radius: 10rpx;
    margin-left: 8rpx;
}

.doctor-title view:nth-child(2) {
    margin-top: 10rpx;
}

.doctor-title view:nth-child(3) {
    font-size: 25rpx;
    color: #8E8E8E;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-top: 12rpx;
}

.doctor-jianjie {
    width: 150rpx;
    padding-left: 20rpx;
    margin-top: 8rpx;
}

.doctor-jianjie image,.doctor-jianjie text {
    display: block;
}

.doctor-jianjie image {
    width: 80rpx;
    height: 80rpx;
    margin: 0 auto;
}

.doctor-jianjie text {
    text-align: center;
    font-size: 22rpx;
    color: #FA8D87;
}

.doctor-jianjie text.onon {
    color: #8E8E8E;
}

.empty {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background: #f6f6f6;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5000;
    padding-top: 300rpx;
}

.empty view:first-child {
    width: 100%;
    height: 360rpx;
}

.empty view:first-child image {
    width: 100%;
    height: 100%;
}

.empty view:last-child {
    width: 100%;
    padding: 10rpx;
    text-align: center;
    font-size: 30rpx;
    color: #bec1c3;
}