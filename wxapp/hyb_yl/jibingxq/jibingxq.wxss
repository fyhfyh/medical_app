}

.gaiaudio {
    width: 80%;
    margin: 0 auto;
    height: 150rpx;
    background: #FCFCFC;
    border-radius: 5rpx;
    display: flex;
    align-items: center;
}

.gaiaudio-imgs {
    width: 150rpx;
    height: 150rpx;
    position: relative;
}

.gaiaudio-imgs .gaiaudio-post {
    width: 100%;
    height: 100%;
}

.gaiaudio-imgs .gaiaudio-btns {
    position: absolute;
    top: 30%;
    left: 22%;
    width: 70rpx;
    height: 70rpx;
    border-radius: 63%;
    border: 4rpx solid #fff;
    padding-top: 12rpx;
    padding-left: 13rpx;
}

.gaiaudio-imgs .gaiaudio-btns>image {
    width: 45rpx;
    height: 45rpx;
}

.gaiaudio-author {
    width: 75%;
    height: 150rpx;
    margin-left: 20rpx;
    padding-top: 35rpx;
}

.gaiaudio-author view:first-child {
    color: #353535;
    font-size: 30rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.gaiaudio-author view:last-child {
    color: #888888;
    font-size: 26rpx;
    margin-top: 15rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}