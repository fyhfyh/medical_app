}

.vidzhetxt {
    width: 100%;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
}

.vidbtn {
    width: 28%;
    margin: 3% auto 0;
    color: #fff;
    background-color: #06ac85;
    padding: 0;
    min-height: 30px;
    height: 30px;
    line-height: 30px;
    font-size: 30rpx;
}

.tab {
    width: 100%;
    background: #fff;
}

.tab text {
    display: inline-block;
    width: 33.33%;
    padding: 20rpx 0;
    text-align: center;
    font-size: 28rpx;
    color: #444;
    border-bottom: 1rpx solid #444;
    letter-spacing: 3rpx;
}

.tab text.onon {
    color: #06ac85;
    border-bottom: 2rpx solid #06ac85;
}

.xq {
    width: 100%;
    padding: 20rpx;
    background: #fff;
}

.xq-kcxx {
    width: 100%;
    font-size: 28rpx;
    color: #444;
    letter-spacing: 3rpx;
    margin-bottom: 15rpx;
    margin-top: 10rpx;
}

.xq-kcxx-box {
    margin-bottom: 20rpx;
}

.xq-kcxx-box text {
    display: block;
    font-size: 25rpx;
    color: #8e8e8e;
    letter-spacing: 3rpx;
    margin-bottom: 15rpx;
}

.js-img image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.js-img text {
    display: inline-block;
    font-size: 28rpx;
    color: #444;
    letter-spacing: 3rpx;
    position: relative;
    top: -30rpx;
    margin: 0;
}

.xq-mu {
    width: 100%;
    overflow: hidden;
}

.xq-mu image {
    float: left;
    width: 50rpx;
    height: 50rpx;
}

.xq-mu text {
    float: left;
    width: 90%;
    font-size: 28rpx;
    color: #444;
    letter-spacing: 3rpx;
}

.xq-mu-vd {
    width: 100%;
    padding-left: 20rpx;
}

.xq-mu-vd-list {
    width: 100%;
    overflow: hidden;
    padding-bottom: 10rpx;
}

.xq-mu-vd-name {
    float: left;
    width: 85%;
}

.xq-mu-vd-name image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
    position: relative;
    top: 7rpx;
}

.xq-mu-vd-name text {
    font-size: 25rpx;
    color: #8e8e8e;
}

.xq-mu-vd-name .c {
    color: #06ac85;
}

.xq-mu-vd-time {
    float: right;
    width: 15%;
    font-size: 25rpx;
    color: #444;
    text-align: right;
    margin-top: 20rpx;
}

.pl {
    width: 100%;
    background: #fff;
}

.pl-txt {
    width: 100%;
    padding: 20rpx;
    background: #eee;
    font-size: 28rpx;
    color: #8e8e8e;
    text-align: center;
}

.pl-txt text {
    padding: 3rpx 10rpx;
    color: #06ac85;
    border: 1rpx solid #06ac85;
    border-radius: 10rpx;
}

.pl-list {
    width: 100%;
    overflow: hidden;
    padding: 20rpx;
    border-bottom: 1rpx solid #ababab;
}

.pl-list:last-child {
    border: none;
}

.pl-left {
    width: 130rpx;
    float: left;
}

.pl-img {
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
}

.pl-img image {
    border-radius: 50%;
}

.pl-name {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 22rpx;
    color: #ababab;
    text-align: center;
    margin-top: 10rpx;
    letter-spacing: 3rpx;
}

.pl-right {
    float: right;
    width: 80%;
}

.pl-right-top {
    width: 100%;
    overflow: hidden;
}

.pl-ping {
    width: 50%;
    float: left;
}

.pl-ping image {
    width: 30rpx;
    height: 30rpx;
    position: relative;
    top: 5rpx;
    margin-right: 10rpx;
}

.pl-ping text {
    font-size: 25rpx;
    color: #ababab;
    letter-spacing: 3rpx;
}

.pl-time {
    float: right;
    width: 50%;
    text-align: right;
    font-size: 25rpx;
    color: #ababab;
    margin-top: 15rpx;
}

.pl-right-bottom {
    width: 100%;
    font-size: 25rpx;
    color: #444;
    letter-spacing: 3rpx;
    line-height: 40rpx;
}

view.show {
    display: block;
}

view.hide {
    display: none;
}

.bq {
    width: 100%;
    text-align: center;
}

.bq text {
    display: inline-block;
    font-size: 22rpx;
    color: #8e8e8e;
}

.bq image {
    display: inline-block;
    width: 25rpx;
    height: 25rpx;
    position: relative;
    top: 5rpx;
}

.footer {
    width: 100%;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    overflow: hidden;
    border-top: 1px solid #eee;
}

.footer view.footer-list {
    float: left;
    width: 22%;
    height: 100rpx;
    text-align: center;
    padding-top: 10rpx;
}

.footer view.footer-list:last-child {
    float: left;
    width: 34%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 28rpx;
    color: #fff;
    background-color: #06ac85;
    padding: 0;
}

.footer view.footer-list .footer-list-img {
    display: block;
    width: 35rpx;
    height: 35rpx;
    margin: 0 auto;
}

.footer-list-title {
    display: block;
    width: 100%;
    text-align: center;
    font-size: 28rpx;
    color: #949494;
    margin-top: 8rpx;
}

button.weixin {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    padding: 0;
    position: fixed;
    bottom: 185rpx;
    right: 20rpx;
    z-index: 1000;
    background: #fff;
}

button.weixin image {
    width: 100%;
    height: 100%;
}

button[plain] {
    border: none;
}

.pl-index {
    width: 100%;
    height: 800rpx;
    padding: 50rpx;
    background: #eee;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
}

.pl-index-form {
    width: 100%;
    padding: 20rpx;
    background: #fff;
}

.form-kc {
    width: 100%;
    overflow: hidden;
    margin-top: 50rpx;
}

.form-kc-left {
    float: left;
    width: 90rpx;
    height: 90rpx;
}

.form-kc-right {
    float: left;
    width: 85%;
    padding-left: 20rpx;
    margin-top: 5rpx;
}

.form-kc-right view {
    width: 100%;
    font-size: 25rpx;
    color: #444;
}

.form-kc-right view:last-child {
    margin-top: 20rpx;
}

.form-kc-right view text {
    color: #fa8d87;
}

.form-group {
    width: 100%;
    overflow: hidden;
    margin-top: 50rpx;
}

.form-group label {
    width: 30%;
    float: left;
    text-align: center;
    border: 1rpx solid #ababab;
    border-radius: 10rpx;
}

.form-group label.onon {
    border: 1rpx solid #65ebd0;
}

.form-group label:nth-child(2) {
    margin: 0 3.5%;
}

.form-group label image {
    width: 30rpx;
    height: 30rpx;
}

.form-group label text {
    font-size: 25rpx;
    color: #444;
    position: relative;
    top: -3rpx;
    margin-left: 5rpx;
}

.form-textarea {
    width: 100%;
    height: 190rpx;
    padding: 10rpx;
    box-sizing: border-box;
    border: 1rpx solid #ababab;
    margin-top: 50rpx;
    border-radius: 10rpx;
    font-size: 25rpx;
    color: #444;
    letter-spacing: 3rpx;
    line-height: 40rpx;
}

.btn {
    width: 100%;
    margin-top: 50rpx;
    margin-bottom: 35rpx;
}

.btn button {
    width: 50%;
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    color: #fff;
    background: #65ebd0;
    font-size: 28rpx;
}

.pl-close {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    bottom: 690rpx;
    right: 60rpx;
}

view.show {
    display: block;
}

view.hide {
    display: none;
}

.sharebtn {
    border: none;
    background-color: #fff;
    padding-left: 0;
    padding-right: 0;
    margin-left: 0;
    margin-right: 0;
    width: 100%;
    border-radius: 0;
    height: 100rpx;
    line-height: 2.1;
}

.sharebtn::after {
    border: none;
    border-radius: 0;
}

.h98 {
    display: flex;
    align-items: center;
    height: 88rpx;
    border-bottom: 2rpx solid #eee;
}

.bdb2 {
    border-bottom: 2rpx solid #eee;
}

.mt8 {
