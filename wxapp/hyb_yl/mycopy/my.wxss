page {
    background: #eee;
    padding-bottom: 10rpx;
}

despage {
    background: #f5f4f4;
    padding-bottom: 80rpx;
}

.jian {
    position: absolute;
    width: 35rpx;
    height: 35rpx;
    top: -15rpx;
    right: -15rpx;
}

.upload_pics .border {
    border-radius: 10rpx;
    width: 150rpx;
    height: 150rpx;
}

.upload_pics {
    display: inline-block;
    width: 150rpx;
    height: 150rpx;
    vertical-align: top;
    margin-left: 20rpx;
    margin-top: 20rpx;
    position: relative;
}

view {
    box-sizing: border-box;
}

.qhtab {
    width: 125rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    z-index: 1000;
}

.icon_img {
    width: 35rpx;
    height: 35rpx;
    vertical-align: middle;
}

.des {
    margin-top: 10rpx;
    padding: 10rpx 30rpx;
    margin-bottom: 10rpx;
    width: 90%;
    height: 130rpx;
    line-height: 1.5;
    border: 2rpx solid #cbcbcb;
    border-radius: 15rpx;
    font-size: 26rpx;
}

.qhtab text {
    display: inline-block;
    width: 45rpx;
    text-align: center;
    padding: 5rpx;
    color: #fff;
    background: #afafaf;
    font-size: 22rpx;
}

.qhtab text:first-child {
    padding-left: 10rpx;
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
}

.qhtab text:last-child {
    padding-right: 10rpx;
    border-top-right-radius: 50rpx;
    border-bottom-right-radius: 50rpx;
}

.qhtab text.onon {
    background: #06AC85;
}

.doctor {
    position: relative;
}

.doctor-back {
    height: 420rpx;
}

.doctor-zhezhao {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 420rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-size: 30rpx;
}

.name {
    margin: 10rpx 0;
}

.doctor-img {
    width: 150rpx;
    height: 150rpx;
    margin-top: 90rpx;
}

.doctor-img image {
    border-radius: 50%;
}

image {
    width: 100%;
    height: 100%;
}

.sec {
    width: 100%;
    padding: 50rpx;
}

.sec>view {
    display: inline-block;
    width: 50%;
    text-align: center;
}

.money {
    color: #ff542c;
    font-size: 25rpx;
    font-weight: bold;
}

.money text {
    font-size: 35rpx;
}

.sec_intro {
    color: #afafaf;
    font-size: 28rpx;
    margin-top: 10rpx;
}

.my_icon {
    width: 42rpx;
    height: 34rpx;
    vertical-align: middle;
    position: relative;
    top: -3rpx;
}

.third view:nth-child(2) image.my_icon {
    width: 37rpx;
    height: 34rpx;
}

.third view:nth-child(6) image.my_icon {
    left: -5rpx;
    width: 48rpx;
    height: 34rpx;
}

.third view:nth-child(7) image.my_icon {
    width: 37rpx;
    height: 34rpx;
}

.third view:nth-child(8) image.my_icon {
    width: 37rpx;
    height: 34rpx;
}

.third view:nth-child(9) image.my_icon {
    width: 36rpx;
    height: 34rpx;
}

.third view:nth-child(10) image.my_icon {
    width: 36rpx;
    height: 34rpx;
}

.my_arrow {
    width: 15rpx;
    height: 26rpx;
    position: absolute;
    top: 35rpx;
    right: 30rpx;
}

.third {
    width: 100%;
    overflow: hidden;
    font-size: 30rpx;
    padding-bottom: 50rpx;
    padding: 0 20rpx;
    position: relative;
    margin-top: 20rpx;
}

.third view {
    width: 100%;
    margin-bottom: 9rpx;
    text-align: left;
    padding-left: 75rpx;
    position: relative;
    margin-top: -5rpx;
    background: #fff;
    padding: 15rpx 25rpx;
}

.third view:nth-child(4),.third view:nth-child(7) {
    margin-top: 25rpx;
}

.third view text {
    margin: 0 10rpx;
}

.third view text.xiaofen {
    position: absolute;
    right: 55rpx;
    top: 26rpx;
    color: #ff582b;
}

view.show {
    display: block;
}

view.hide {
    display: none;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-top: 1rpx solid #c5c5c5;
}

.footer view {
    display: inline-block;
    width: 25%;
}

.footer view image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
}

.footer view text {
    display: block;
    width: 100%;
    font-size: 25rpx;
    color: #afafaf;
    text-align: center;
    margin-top: 5rpx;
}

.box {
    width: 100%;
    padding: 20rpx;
    background: #fff;
}

.form-group {
    margin-bottom: 20rpx;
    overflow: hidden;
    position: relative;
}

.form-group-top {
    width: 100%;
    overflow: hidden;
    margin-top: 20rpx;
}

.form-group-top image {
    display: inline-block;
    width: 35rpx;
    height: 35rpx;
}

.form-group-top text {
    display: inline-block;
    font-size: 28rpx;
    color: #444;
    font-weight: bold;
    position: relative;
    top: -5rpx;
    margin-left: 10rpx;
}

.form-group input {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    border: 1rpx solid #eee;
    font-size: 25rpx;
    color: #444;
}

.form-group-xb {
    width: 100%;
    margin-top: 5rpx;
    overflow: hidden;
}

.form-group-xb label {
    float: left;
    width: 10%;
    padding: 5rpx 10rpx;
    border: 1rpx solid #eee;
    border-radius: 10rpx;
    overflow: hidden;
    margin-right: 20rpx;
}

.form-group-xb label image {
    float: left;
    width: 35rpx;
    height: 35rpx;
    margin-top: 2rpx;
}

.form-group-xb label text {
    float: left;
    font-size: 28rpx;
    color: #a5a5a5;
    margin-left: 5rpx;
}

.form-group-xb label.onon {
    background: #4acab3;
}

.form-group-xb label.onon text {
    color: #fff;
}

.zuojiantou {
    position: absolute;
    top: 75rpx;
    right: 0;
    width: 50rpx;
    height: 55rpx;
}

.btn {
    width: 100%;
    padding: 50rpx 0 120rpx 0;
}

.btn button {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background: #4acab3;
    font-size: 28rpx;
    color: #fff;
}

button::after {
    border: none;
}

view.border {
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    border: 1rpx solid #eee;
    font-size: 25rpx;
    color: #444;
    margin-top: 10rpx;
}

.form-group-money {
    position: relative;
    float: right;
    width: 20%;
    margin-top: 15rpx;
}

.form-group-money text {
    font-size: 25rpx;
    color: #ff582b;
    position: absolute;
    top: 15rpx;
    left: 20rpx;
}

.form-group-money input {
    padding-left: 42rpx;
    color: #ff582b;
    border: none;
    font-size: 30rpx;
    font-weight: bold;
}

.title {
    font-weight: bold;
    font-size: 26rpx;
    margin-left: 10rpx;
}

image.show {
    display: inline-block;
}

image.hide {
    display: none;
}

.upload_pic {
    display: inline-block;
    width: 150rpx;
    height: 150rpx;
    vertical-align: top;
    margin-left: 20rpx;
    margin-top: 20rpx;
    position: relative;
}

.upload_pic .jian {
    position: absolute;
    width: 35rpx;
    height: 35rpx;
    top: -15rpx;
    right: -15rpx;
}

.upload_pic image {
    border-radius: 10rpx;
}

.bq {
    width: 100%;
    text-align: center;
}

.bq text {
    display: inline-block;
    font-size: 22rpx;
    color: #8e8e8e;
}

.bq image {
    display: inline-block;
    width: 25rpx;
    height: 25rpx;
    position: relative;
    top: 5rpx;
}

.third .w50 {
    display: inline-block;
    width: 8%;
    padding: 0;
    margin: 0;
}