page {
    background: #F5F4F4;
    padding-bottom: 110rpx;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.taocan {
    width: 100%;
    background: #fff;
    border-bottom: 1px solid #F1F1F1;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
}

.taocan view {
    padding: 15rpx 20rpx;
    display: inline-block;
}

.taocan view text:first-child {
    font-size: 26rpx;
    color: #4E4E4E;
}

.taocan view text:last-child {
    margin: 0 auto;
    display: block;
    width: 30rpx;
    border-bottom: 5rpx solid transparent;
    position: relative;
    top: 15rpx;
    z-index: 11111;
}

.taocan view.onon text:first-child {
    font-size: 31rpx;
    color: #000;
    font-weight: bold;
}

.taocan view.onon text:last-child {
    border-bottom: 5rpx solid #242424;
}

.serve {
    width: 100%;
    padding: 0 20rpx;
    background: #fff;
    margin-top: 85rpx;
}

.serve-bottom {
    width: 100%;
    overflow: hidden;
}

.list-title {
    width: 100%;
    font-size: 28rpx;
    color: #444;
    padding: 20rpx 0;
}

.p0 {
    padding: 0;
}

.list-imgs {
    display: inline-block;
    width: 31.8%;
    height: 225rpx;
}

.list-tiem {
    padding: 10rpx 0;
    border-bottom: 1rpx solid #efefef;
    overflow: hidden;
}

.list-tiem text:first-child {
    float: left;
    font-size: 22rpx;
    color: #FF582B;
    width: 78%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list-tiem text:first-child.c {
    color: #06AC85;
}

.list-tiem text:last-child {
    float: right;
    width: 20%;
    font-size: 22rpx;
    color: #ABABAB;
    text-align: right;
}

.list-imgss view.list-imgs:nth-child(2) {
    margin: 0 15rpx;
}

view.show {
    display: block;
}

view.hide {
    display: none;
}

.wu {
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}