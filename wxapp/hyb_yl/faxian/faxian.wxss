page {
    background: #eee;
    padding-bottom: 10rpx;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.link {
    width: 100%;
    padding: 20rpx;
    padding-left: 0;
    background: #fff;
    overflow: hidden;
}

.tips1 view {
    width: 100%;
    padding: 10rpx 0 20rpx 0;
    text-align: center;
    box-sizing: border-box;
    font-family: "微软雅黑";
    font-size: 28rpx;
    color: #8e8e8e;
}

.link-list {
    float: left;
    width: 47%;
    margin-left: 20rpx;
    padding: 30rpx;
    background: #FFF2F1;
    border-radius: 10rpx;
}

.link-list:last-child {
    background: #F5F9E4;
}

.link-list view {
    font-size: 28rpx;
    color: #242424;
    font-weight: bold;
    position: relative;
    letter-spacing: 3rpx;
}

.link-list view image {
    width: 45rpx;
    height: 45rpx;
    position: absolute;
    top: -2rpx;
    left: 120rpx;
}

.link-list view:last-child {
    font-size: 25rpx;
    color: #999;
    margin-top: 18rpx;
}

.nav {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    margin-top: 20rpx;
}

.nav view {
    display: inline-block;
    margin-left: 20rpx;
    text-align: center;
}

.nav view:first-child {
    margin-left: 0;
}

.bdb2.c {
    border-color: #06AC85;
}

.questions {
    width: 100%;
    background: #fff;
    margin-top: 20rpx;
}

.questions-title {
    width: 100%;
    padding: 20rpx;
    text-align: center;
    font-size: 28rpx;
    color: #242424;
    font-weight: bold;
    border-bottom: 1rpx solid #eee;
}

.doctor-bottom {
    width: 100%;
}

.doctor-bottom-list {
    width: 100%;
    padding: 25rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #eee;
}

.fudai-imgs {
    width: 100%;
    padding: 20rpx 0;
}

.fudai-imgs image {
    width: 110rpx;
    height: 110rpx;
    margin-left: 10rpx;
}

.fudai-imgs image:first-child {
    margin-left: 0;
}

.doctor-touxiangimg {
    width: 105rpx;
    height: 105rpx;
    float: left;
}

.doctor-rightcontent {
    float: right;
    width: 82%;
    padding-top: 5rpx;
    position: relative;
}

.doctor-username {
    font-size: 30rpx;
    color: #06AC85;
    margin-bottom: 15rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    letter-spacing: 3rpx;
}

.doctor-wenti {
    font-size: 28rpx;
    color: #242424;
    letter-spacing: 3rpx;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    word-break: break-all;
}

.doctor-imgs {
    width: 100%;
    margin-top: 20rpx;
}

.doctor-imgs image {
    width: 170rpx;
    height: 160rpx;
    margin-top: 0rpx;
}

.doctor-imgs image:nth-child(2),.doctor-imgs image:nth-child(5) {
    margin: 0 10rpx;
}

.doctor-huida {
    width: 200rpx;
    position: absolute;
    top: 6rpx;
    right: 0;
}

.questions-zan {
    float: right;
    width: 35%;
    font-size: 25rpx;
    color: #242424;
    text-align: right;
    margin-top: -3rpx;
    margin-left: 35rpx;
}

.questions-zan image {
    width: 45rpx;
    height: 45rpx;
}

.questions-zan text {
    position: relative;
    top: 10rpx;
    right: 0;
    color: #AFAFAF;
    font-size: 30rpx;
}

.doctor-askhuida {
    width: 100%;
    padding: 10rpx;
    background: #eee;
    position: relative;
    border-radius: 5rpx;
    margin-top: 20rpx;
}

.doctor-askhuida-sanjiao {
    width: 0;
    height: 0;
    border-left: 20rpx solid transparent;
    border-right: 20rpx solid transparent;
    border-bottom: 20rpx solid #eee;
    position: absolute;
    top: -15rpx;
    left: 20rpx;
}

.doctor-askhuida-content {
    width: 100%;
    overflow: hidden;
}

.doctor-asd {
    width: 100%;
    overflow: hidden;
    color: #000;
    font-size: 28rpx;
    word-break: break-all;
}

.doctor-asd navigator {
    display: inline-block;
}

.doctor-asd image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    position: relative;
    top: 10rpx;
}

.doctor-asd text {
    display: inline-block;
    font-size: 28rpx;
    color: #B8B8B8;
    margin-left: 10rpx;
}

.doctor-asd text.txt-xian {
    width: 6rpx;
    height: 6rpx;
    background: #06AC85;
    border-radius: 50%;
    position: relative;
    top: -4rpx;
}

.doctor-wen {
    width: 100%;
    max-height: 120rpx;
    font-size: 28rpx;
    color: #8E8E8E;
    letter-spacing: 3rpx;
    line-height: 40rpx;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-top: 1rpx solid #C5C5C5;
}

.footer view {
    display: inline-block;
    width: 25%;
}

.footer view image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
}

.footer view text {
    display: block;
    width: 100%;
    font-size: 25rpx;
    color: #AFAFAF;
    text-align: center;
    margin-top: 5rpx;
}

.bq {
    width: 100%;
    text-align: center;
}

.bq text {
    display: inline-block;
    font-size: 22rpx;
    color: #8e8e8e;
}

.bq image {
    display: inline-block;
    width: 25rpx;
    height: 25rpx;
    position: relative;
    top: 5rpx;
}

.h72 {
    height: 72rpx;
}

.wenda {
    width: 80rpx;
    height: 30rpx;
    line-height: 30rpx;
    margin: 0;
    padding: 0;
    background-color: transparent;
}

.wenda::after {
    border-color: #8e8e8e;
}

.h88 {
    height: 88rpx;
    line-height: 88rpx;
}

.h58 {
    height: 58rpx;
}