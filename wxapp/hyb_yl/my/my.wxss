.mt60 {
    margin-top: 60rpx;
}

.none {
    color: #9698a4;
    background-color: #fff;
}

.nav {
    position: absolute;
    top: 291rpx;
    width: 100%;
    z-index: 2;
}

.nav-scr {
    height: 142rpx;
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.mt122 {
    margin-top: 122rpx;
}

.gongju {
    width: 25%;
    margin-bottom: 25rpx;
}

.dangan-btn {
    padding: 15rpx 60rpx;
    border: 1rpx solid #02b3b9;
}

.fs40 {
    font-size: 40rpx;
}

.bdl {
    border-left: 1rpx solid #ebebeb;
}

.footer {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 10rpx;
    box-sizing: border-box;
    background: #fff;
    border-top: 1rpx solid #C5C5C5;
}

.footer view {
    display: inline-block;
    width: 25%;
}

view {
    box-sizing: border-box;
}

.footer view image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 0 auto;
}

.footer view text {
    display: block;
    width: 100%;
    font-size: 25rpx;
    color: #AFAFAF;
    text-align: center;
    margin-top: 5rpx;
}

.form-group {
    margin-bottom: 20rpx;
    overflow: hidden;
    position: relative;
}

.form-group-money {
    position: relative;
    float: right;
    width: 20%;
    margin-top: 15rpx;
}

.form-group-money text {
    font-size: 25rpx;
    color: #ff582b;
    position: absolute;
    top: 15rpx;
    left: 20rpx;
}

.btn {
    width: 100%;
    padding: 50rpx 0 120rpx 0;
}

.btn button {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background: #4acab3;
    font-size: 28rpx;
    color: #fff;
}

.form-group-money input {
    padding-left: 42rpx;
    color: #ff582b;
    border: none;
    font-size: 30rpx;
    font-weight: bold;
}

.form-group-top {
    width: 100%;
    overflow: hidden;
    margin-top: 20rpx;
}

.form-group-top image {
    display: inline-block;
    width: 35rpx;
    height: 35rpx;
}

.form-group-top text {
    display: inline-block;
    font-size: 28rpx;
    color: #444;
    font-weight: bold;
    position: relative;
    top: -5rpx;
    margin-left: 10rpx;
}

.form-group input {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    border-radius: 10rpx;
    border: 1rpx solid #eee;
    font-size: 25rpx;
    color: #444;
}

.form-group-xb {
    width: 100%;
    margin-top: 5rpx;
    overflow: hidden;
}

.form-group-xb label {
    float: left;
    width: 10%;
    padding: 5rpx 10rpx;
    border: 1rpx solid #eee;
    border-radius: 10rpx;
    overflow: hidden;
    margin-right: 20rpx;
}

.form-group-xb label image {
    float: left;
    width: 35rpx;
    height: 35rpx;
    margin-top: 2rpx;
}

.form-group-xb label text {
    float: left;
    font-size: 28rpx;
    color: #a5a5a5;
    margin-left: 5rpx;
}

.form-group-xb label.onon {
    background: #4acab3;
}

.form-group-xb label.onon text {
    color: #fff;
}

.zi2000 {
    z-index: 2000;
}