    height: 150rpx;
}

.title {
    font-weight: bold;
    font-size: 26rpx;
    margin-left: 10rpx;
}

.header-bg {
    width: 160rpx;
    height: 160rpx;
}

.my-btn-l {
    height: 43rpx;
    padding: 10rpx 15rpx;
    background-color: #1bd9ac;
