<view class="yiyuanbox">
    <view class="yiyuanboxleft">
        <view bindtap="selyiyuan" class="{{current==index?'onon':''}}" data-index="{{index}}" wx:for="{{hsp}}" wx:key="yytypes">{{item.dz_name}}</view>
    </view>
    <view class="yiyuanboxright">
        <view bindtap="success" data-yiyuan="{{item.yname}}" wx:for="{{hsp[current].items}}" wx:key="list">{{item.yname}}</view>
    </view>
</view>
