page {
    background-color: #efefef;
}

.prediagnosis_search {
    border-top: 2rpx solid #d8d8d8;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
}

.prediagnosis_search_bg {
    display: flex;
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
}

.prediagnosis_search_bg input {
    display: inline-block;
    width: 96%;
    font-size: 28rpx;
    color: #e7e7e7;
    margin-left: 5rpx;
    text-align: center;
}

.ce {
    color: #cecece;
}

.prediagnosis {
    display: flex;
    justify-content: space-between;
    background-color: #fff;
    box-sizing: border-box;
    padding: 24rpx;
}

.prediagnosis_l {
    display: flex;
    align-items: center;
}

.prediagnosis_r {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding-left: 30rpx;
    margin-left: 30rpx;
    border-left: 2rpx solid #e0e0e0;
}

.prediagnosis_con {
    margin-left: 15rpx;
}

.prediagnosis .cir::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    right: -5rpx;
    width: 15rpx;
    height: 15rpx;
    border-radius: 50%;
    background-color: #f60500;
}

.prediagnosis_title {
    font-size: 30rpx;
    color: #000;
}

.prediagnosis_explain {
    font-size: 22rpx;
    color: #858585;
    margin-top: 10rpx;
    line-height: 32rpx;
}

.prediagnosis .pr {
    position: relative;
    display: flex;
    align-items: center;
}

.quick_check {
    margin-top: 18rpx;
    background-color: #fff;
    box-sizing: border-box;
    padding: 30rpx 40rpx 40rpx 24rpx;
}

.title {
    font-size: 28rpx;
    color: #000;
    border-left: 10rpx solid #0bac98;
    padding-left: 24rpx;
}

.con {
    display: flex;
    flex-wrap: wrap;
    box-sizing: border-box;
    padding: 0 15rpx;
}

.item {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    width: 110rpx;
    font-size: 26rpx;
    color: #000;
    margin-right: 73rpx;
    margin-top: 40rpx;
}

.item:nth-child(4n) {
    margin-right: 0;
}

.item text {
    margin-top: 30rpx;
}

.h88 {
    height: 88rpx;
}

.w470 {
    width: 470rpx;
}

.h200 {
    height: 200rpx;
}

.h34 {
    height: 34rpx;
}

.w106 {
    width: 106rpx;
}

.p0 {
    padding: 0;
}

.mr20:nth-last-child(1) {
    margin-right: 0;
}

.copy {
    height: 78rpx;
    line-height: 78rpx;
    text-align: center;
    font-size: 28rpx;
    margin-top: 18rpx;
    color: #858585;
}