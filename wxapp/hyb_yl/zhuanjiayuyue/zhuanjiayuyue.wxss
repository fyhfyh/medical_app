page {
    background: #fff;
    padding-bottom: 100rpx;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.box {
    width: 100%;
    height: 420rpx;
    position: relative;
}

.background {
    width: 100%;
    height: 420rpx;
}

.beijing {
    position: absolute;
    width: 100%;
    height: 420rpx;
    top: 0;
    left: 0;
    padding-top: 30rpx;
}

.box-zjjs {
    position: absolute;
    width: 100%;
    height: 420rpx;
}

.zjjs-touxiang {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    margin: 10rpx auto;
}

.zjjs-touxiang image {
    border-radius: 50%;
}

.zjjs-name,.zjjs-yy {
    text-align: center;
    color: #fff;
    margin-top: 7rpx;
    letter-spacing: 3rpx;
}

.zjjs-name {
    font-size: 30rpx;
}

.zjjs-yy {
    font-size: 22rpx;
}

.zjjs-num {
    margin-top: 20rpx;
}

.zjjs-num view {
    width: 49%;
    display: inline-block;
}

.zjjs-num view text {
    display: block;
    color: #fff;
    font-size: 32rpx;
    text-align: center;
    letter-spacing: 3rpx;
}

.zjjs-num view text:last-child {
    font-size: 22rpx;
    margin-top: 10rpx;
}

.zjjs-num view.zjjs-xian {
    width: 1rpx;
    display: inline-block;
    height: 60rpx;
    border-left: 1rpx solid #FFA099;
}

.times-title {
    width: 100%;
    padding: 20rpx 0;
    text-align: center;
    font-size: 30rpx;
    color: #06AC85;
    border-bottom: 5rpx solid #06AC85;
}

.times-date {
    width: 100%;
    text-align: center;
    padding: 20rpx 0;
    font-size: 30rpx;
    color: #242424;
}

.times-poke {
    width: 100%;
    padding: 20rpx;
}

.times-poke-title {
    width: 100%;
    text-align: center;
    padding: 20rpx 0;
    font-size: 28rpx;
    color: #06AC85;
}

.times-time {
    width: 100%;
    padding: 50rpx;
    padding-top: 0;
}

.times-time view {
    width: 100%;
    overflow: hidden;
    margin-bottom: 10rpx;
}

.times-time view text:first-child {
    float: left;
    width: 50%;
    text-align: center;
    font-size: 28rpx;
    color: #242424;
    padding: 10rpx 0;
}

.times-time view text:last-child {
    float: right;
    width: 50%;
    background: #06AC85;
    color: #fff;
    font-size: 28rpx;
    border-radius: 10rpx;
    text-align: center;
    padding: 10rpx 0;
}

.copyright {
    width: 100%;
}

.copyright text {
    width: 100%;
    display: block;
    text-align: center;
    color: #B8B8B8;
    letter-spacing: 3rpx;
    margin-bottom: 5rpx;
    font-size: 25rpx;
}