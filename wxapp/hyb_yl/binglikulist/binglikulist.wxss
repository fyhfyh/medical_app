page {
    background: #fff;
}

view {
    box-sizing: border-box;
}

image {
    width: 100%;
    height: 100%;
}

.box {
    width: 100%;
    padding-left: 20rpx;
}

.box-list {
    width: 100%;
    padding: 20rpx;
    padding-left: 0;
    border-bottom: 1px solid #F1F1F1;
    overflow: hidden;
    position: relative;
}

.list-left {
    float: left;
    width: 130rpx;
    height: 130rpx;
    border-radius: 10rpx;
}

.list-left image {
    border-radius: 10rpx;
}

.list-right {
    width: 68%;
    float: left;
    padding-left: 20rpx;
}

.list-right view {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 30rpx;
    color: #46c7bf;
    margin-bottom: 8rpx;
}

.list-right view:nth-child(2) {
    font-size: 25rpx;
    color: #afafaf;
}

.list-right view:last-child {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 0;
    font-size: 28rpx;
    color: #3e3e3e;
}

.laji {
    float: right;
    width: 90rpx;
    height: 100rpx;
    margin-top: 15rpx;
}

.empty {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background: #f6f6f6;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 5000;
    padding-top: 300rpx;
}

.empty view:first-child {
    width: 100%;
    height: 360rpx;
}

.empty view:first-child image {
    width: 100%;
    height: 100%;
}

.empty view:last-child {
    width: 100%;
    padding: 10rpx;
    text-align: center;
    font-size: 30rpx;
    color: #bec1c3;
}