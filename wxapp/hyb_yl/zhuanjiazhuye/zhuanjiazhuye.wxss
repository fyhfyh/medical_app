    border-bottom: 1rpx solid #f6f6f6;
}

.doctor-time view:last-child {
    width: 100%;
    overflow: hidden;
    padding: 20rpx 0;
}

.doctor-time view:last-child text {
    width: 100rpx;
    height: 40rpx;
