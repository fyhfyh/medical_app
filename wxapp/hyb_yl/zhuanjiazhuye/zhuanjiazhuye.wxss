    text-align: center;
    border-radius: 10rpx;
    background: #06ac85;
    color: #fff;
    font-size: 25rpx;
    margin-top: 12rpx;
}

.doctor-bottom-list {
    width: 100%;
    padding: 20rpx 0;
}

.doctor-time {
    width: 100%;
}

.doctor-time view:first-child {
    font-size: 28rpx;
    color: #444;
