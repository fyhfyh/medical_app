    line-height: 40rpx;
    float: left;
    font-size: 28rpx;
    color: #b8b8b8;
    margin-left: 20rpx;
}

.doctor-time view:last-child text:nth-child(1),.doctor-time view:last-child text:nth-child(5) {
    margin-left: 0;
}

