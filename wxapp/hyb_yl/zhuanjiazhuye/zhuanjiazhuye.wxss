.keshi {
    width: 200rpx;
    height: 200rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.keshi text {
    display: block;
    width: 160rpx;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    transform: rotate(47deg);
    position: absolute;
    top: 60rpx;
    right: 0rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.doctor {
    width: 100%;
