    top: -2rpx;
}

.doctor-bottom-jianjie {
    width: 100%;
    overflow: hidden;
}

.doctor-bottom-jianjie text {
    font-size: 28rpx;
}

.doctor-bottom-jianjie text:first-child {
    float: left;
    width: 15%;
    padding: 20rpx;
    color: #8e8e8e;
    letter-spacing: 3rpx;
    padding-left: 15rpx;
    padding-right: 0rpx;
}

.doctor-bottom-jianjie text:nth-child(2) {
    float: left;
    width: 82%;
    padding: 20rpx 0;
    color: #8e8e8e;
    letter-spacing: 3rpx;
    line-height: 40rpx;
}

.doctor-bottom-jianjie text.doctor-yuyue {
    float: right;
    width: 30%;
