page {
    background-color: #efefef;
}

.prediagnosis_search {
    border-top: 2rpx solid #d8d8d8;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
}

.prediagnosis_search_bg {
    display: flex;
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
}

.prediagnosis_search_bg input {
    display: inline-block;
    width: 96%;
    font-size: 28rpx;
    color: #cecece;
    margin-left: 5rpx;
    text-align: center;
}

.ce {
    color: #cecece;
}

.parts {
    display: flex;
    background-color: #fff;
    font-size: 32rpx;
}

.title {
    width: 34%;
    background-color: #f7f7f7;
}

.bdl {
    height: 80rpx;
    line-height: 80rpx;
    border-left: 6rpx solid #0bac98;
    padding-left: 30rpx;
    color: #0bac98;
    background-color: #fff;
}

.pl30 {
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 30rpx;
}