.prediagnosis_search {
    border-top: 2rpx solid #d8d8d8;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
    background-color: #efefef;
}

.prediagnosis_search_bg {
    display: flex;
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
}

.prediagnosis_search_bg input {
    display: inline-block;
    width: 85%;
    font-size: 28rpx;
    color: #cecece;
    margin-left: 15rpx;
}

.prediagnosis_search_bg button {
    width: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 50%;
    margin: 0;
    padding: 0rpx;
    color: #fff;
    text-align: center;
    background-color: #888;
    font-size: 24rpx;
}

.prediagnosis_search_bg button::after {
    border: none;
}

.ce {
    color: #cecece;
}

.list {
    padding-left: 24rpx;
}

.title {
    font-size: 32rpx;
    padding: 10rpx 0;
    border-bottom: 2rpx solid #eee;
    color: #000;
    font-weight: bold;
}

.con {
    font-size: 28rpx;
    padding: 10rpx 0;
    color: #000;
}

.tac {
    margin-top: 20rpx;
    text-align: center;
    font-size: 28rpx;
    color: #999;
}