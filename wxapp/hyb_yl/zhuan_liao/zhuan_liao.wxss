}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    border: transparent;
}

.nomore {
    line-height: 80rpx;
    height: 80rpx;
    text-align: center;
    position: relative;
    font-size: 30rpx;
    width: 100%;
    background: #f2f2f2;
    color: #dbdbdb;
}

.nomore::before {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #dbdbdb;
    position: absolute;
    left: 100rpx;
    top: 50%;
}

.nomore::after {
    content: "";
    display: block;
    width: 100rpx;
    height: 2rpx;
    background: #dbdbdb;
    position: absolute;
    right: 100rpx;
    top: 50%;
}

.liao-footer {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}

.liao-input {
    width: 85%;
    height: 66rpx;
    background-color: #f5f5f5;
    border: 2rpx solid #e4e4e4;
}

.fa-btn {
    display: inline-block;
    width: 85rpx;
    height: 54rpx;
    line-height: 54rpx;
}

.pb80 {
    padding-bottom: 80rpx;
}

.bgc6e7d94 {
    background-color: #6e7d94;
}

.wbba {
    word-break: break-all;
