page {
    position: relative;
    height: 100%;
    background-color: #f2f2f2;
}

.report-header {
    top: 0;
    left: 0;
}

.report-head {
    top: 50%;
    right: 36rpx;
    transform: translate(0,-50%);
    z-index: 2;
}

.w190 {
    width: 190rpx;
}

.h50 {
    height: 50rpx;
}

.zi2 {
    z-index: 2;
}

.h88 {
    height: 88rpx;
}

.mtn35 {
    margin-top: -35rpx;
}

.mt2 {
    margin-top: 2rpx;
}

.w200 {
    width: 200rpx;
}

.h56 {
    height: 56rpx;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.5);
    z-index: 10;
}

.report-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    min-height: 100rpx;
    background-color: #fff;
    z-index: 12;
}

.h98 {
    height: 98rpx;
}

.cir {
    position: relative;
    width: 32rpx;
    height: 32rpx;
    border: 2rpx solid #c2c2c2;
    border-radius: 50%;
}

.cir.hover {
    border-color: #06AC85;
}

.cir.hover::after {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 16rpx;
    height: 16rpx;
    background-color: #06AC85;
    border-radius: 50%;
}

.bdc {
    width: 88rpx;
    height: 40rpx;
    border: 2rpx solid #02b3b9;
    border-radius: 50rpx;
}

.bdl2 {
    border-left: 2rpx solid #fff;
}

.mh412 {
    min-height: 412rpx;
}