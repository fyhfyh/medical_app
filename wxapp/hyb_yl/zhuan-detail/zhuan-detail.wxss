page {
    position: relative;
    height: 100%;
    background-color: #f2f2f2;
}

.my-bg {
    width: 100%;
    height: 329rpx;
}

.header-bg {
    width: 160rpx;
    height: 160rpx;
    background-color: rgba(0,0,0,0.2);
}

.my-btn-l {
    height: 43rpx;
    padding: 10rpx 15rpx;
    background-color: #00c8ce;
    border-radius: 50rpx;
}

.mt60 {
    margin-top: 60rpx;
}

.none {
    color: #9698a4;
    background-color: #fff;
}

.nav {
    position: absolute;
    top: 291rpx;
    width: 100%;
    z-index: 2;
}

.nav-scr {
    height: 142rpx;
    box-shadow: 1rpx 1rpx 30rpx 0.5rpx #e2e2e2;
}

.bdl {
    border-left: 1rpx solid #ebebeb;
}

.lh40 {
    line-height: 40rpx;
}

.ling-btn {
    padding: 10rpx 25rpx;
    background-color: #f2f2f2;
}

.h300 {
    height: 300rpx;
}

.week {
    width: 600rpx;
    height: auto;
}

.week-date text {
    display: inline-block;
    width: 70rpx;
}

.yu-btn {
    width: 68rpx;
    height: 68rpx;
    line-height: 68rpx;
}

.m-btn {
    display: inline-block;
    height: 68rpx;
    line-height: 68rpx;
}

.cir {
    display: inline-block;
    width: 12rpx;
    height: 12rpx;
}

.cir.hover {
    width: 24rpx;
    height: 12rpx;
    background-color: #02b3b9;
    border-radius: 50rpx;
}

.bdl6 {
    border-left: 3rpx solid #2c2e43;
}

.w86 {
    width: 86%;
}

.pf {
    bottom: 0;
    left: 0;
}

.mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
    z-index: 2;
    overflow: hidden;
}

.yuyue {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 600rpx;
    min-height: 100rpx;
    transform: translate(-50%,-50%);
    background-color: #fff;
    z-index: 3;
}

.time-btn {
    width: 238rpx;
    border: 1rpx solid #ebebeb;
    padding: 15rpx 30rpx;
}

.time-btn.hover {
    border: 1rpx solid #02b3b9;
}

.time-btn.c9 {
    color: #ebebeb;
}

.time-btn.none {
    border: 1rpx solid #ebebeb;
}

.pay-btn {
    display: inline-block;
    height: 88rpx;
    line-height: 88rpx;
}

.close {
    left: 0;
    right: 0;
    width: 64rpx;
    margin: 36rpx auto 0;
}

.detail-footer {
    position: fixed;
    bottom: 0rpx;
    left: 0;
    width: 100%;
    height: 98rpx;
    border-top: 2rpx solid #ebebeb;
}

.liao-input {
    width: 85%;
    height: 66rpx;
    background-color: #f5f5f5;
    border: 2rpx solid #e4e4e4;
}

.fa-btn {
    display: inline-block;
    width: 85rpx;
    height: 54rpx;
    line-height: 54rpx;
}

.return-top {
    position: fixed;
    right: 5%;
    bottom: 20%;
    width: 120rpx;
    height: 120rpx;
}

.zi_btn {
    width: 146rpx;
    line-height: normal;
    background-color: transparent;
    margin: 0;
    padding: 5rpx 0;
    box-sizing: content-box;
}

.zi_btn::after {
    border-color: #ffc832;
    border-radius: 50rpx;
}

.mr27 {
    margin-right: 27rpx;
}

.mr27:nth-child(3n) {
    margin-right: 0;
}

.h80 {
    height: 80rpx;
    line-height: 80rpx;
}